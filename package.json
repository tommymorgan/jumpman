{
	"name": "jumpman",
	"displayName": "Jumpman",
	"description": "Jump vertically across space-delimited blocks. Based on Space Block Jumper by jmfirth.",
	"license": "MIT",
	"repository": "github:tommymorgan/jumpman",
	"version": "1.0.0",
	"publisher": "tommymorgan",
	"packageManager": "pnpm@10.15.1+sha512.34e538c329b5553014ca8e8f4535997f96180a1d0f614339357449935350d924e22f8614682191264ec33d1462ac21561aff97f6bb18065351c162c7e8f6de67",
	"engines": {
		"vscode": "^1.79.0",
		"node": ">=24.0.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [],
	"main": "./dist/extension.js",
	"browser": "./dist/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "jumpman.moveUp",
				"title": "Jumpman: Move Up"
			},
			{
				"command": "jumpman.moveDown",
				"title": "Jumpman: Move Down"
			},
			{
				"command": "jumpman.selectUp",
				"title": "Jumpman: Select Up"
			},
			{
				"command": "jumpman.selectDown",
				"title": "Jumpman: Select Down"
			}
		],
		"keybindings": [
			{
				"command": "jumpman.moveUp",
				"key": "ctrl+up",
				"mac": "alt+up",
				"when": "editorTextFocus"
			},
			{
				"command": "jumpman.moveDown",
				"key": "ctrl+down",
				"mac": "alt+down",
				"when": "editorTextFocus"
			},
			{
				"command": "jumpman.selectUp",
				"key": "shift+ctrl+up",
				"mac": "shift+alt+up",
				"when": "editorTextFocus"
			},
			{
				"command": "jumpman.selectDown",
				"key": "shift+ctrl+down",
				"mac": "shift+alt+down",
				"when": "editorTextFocus"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "pnpm run package",
		"compile": "node esbuild.ts",
		"package": "node esbuild.ts --production",
		"build:vsix": "vsce package --no-dependencies",
		"lint": "biome check --write src",
		"format": "biome format --write src",
		"pretest": "pnpm run compile && tsc -p tsconfig.test.json",
		"test": "xvfb-run -a node out/src/test/runTest.js"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.2.2",
		"@types/mocha": "^10.0.10",
		"@types/node": "^24.0.0",
		"@types/vscode": "^1.79.0",
		"@vscode/test-electron": "^2.3.2",
		"@vscode/vsce": "^3.6.0",
		"esbuild": "^0.25.9",
		"glob": "^11.0.3",
		"mocha": "^11.7.2",
		"typescript": "^5.9.2"
	}
}
